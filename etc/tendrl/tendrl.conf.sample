[bridge_common]

# Central store etcd port
etcd_port = 2379

# Central store etcd host/ip
etcd_connection = 0.0.0.0

# Path to log file
log_path = /var/log/tendrl/bridge_common.log
log_level = DEBUG


[node_agent]
# Path to log file
log_path = /var/log/tendrl/node_agent.log
log_level = DEBUG
tendrl_exe_file_prefix = /tmp/.tendrl_runner